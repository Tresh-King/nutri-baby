{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/assets/(.*)",
            "destination": "/frontend/dist/assets/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/index.html"
        }
    ],
    "functions": {
        "api/**/*.ts": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}